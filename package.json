{
  "name": "web-playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "multi-static-dev --config ./configs/main.config.js",
    "build": "multi-static-build --config ./configs/main.config.js",
    "prettier": "prettier-eslint --write \"./**/*.{js,json,css,scss}\"",
    "lint": "eslint src/**/*.js",
    "postinstall": "node ./postinstall.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "suppress-exit-code eslint --fix",
      "git add"
    ],
    "{additional,scripts}/**/*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "prettier --write",
      "suppress-exit-code stylelint --fix",
      "git add"
    ],
    "*.{json,js,ts,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-perf": "3.3.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "localhost-certs": "1.0.0",
    "mocker-api": "1.11.2",
    "prettier": "2.2.1",
    "source-map-explorer": "2.5.2",
    "stylelint": "13.9.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "suppress-exit-code": "1.0.0",
    "tslib": "2.0.0",
    "typescript-plugin-css-modules": "3.1.0"
  },
  "dependencies": {
    "@babel/core": "7.8.4",
    "@babel/eslint-parser": "7.15.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@babel/runtime": "7.8.4",
    "autoprefixer": "9.7.6",
    "babel-loader": "8.2.2",
    "colors-cli": "1.0.27",
    "core-js": "3.15.2",
    "css-loader": "4.3.0",
    "fs-extra": "10.0.0",
    "glslify-loader": "^2.0.0",
    "jquery": "^3.6.0",
    "klaw-sync": "6.0.0",
    "localhost-certs": "1.0.0",
    "lodash": "4.17.21",
    "mocker-api": "1.11.2",
    "multi-static": "0.1.11",
    "mustache": "4.2.0",
    "node-sass": "4.12.0",
    "npm-install-subfolders": "0.1.2",
    "postcss": "8.3.5",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "4.2.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "promise-polyfill": "8.2.0",
    "raw-loader": "^4.0.2",
    "read-first-line": "1.0.4",
    "sass-loader": "10.2.0",
    "shelljs": "0.8.5",
    "static-hash-version": "1.0.0",
    "style-loader": "1.3.0",
    "terser-webpack-plugin": "2.3.4",
    "twgl.js": "^4.19.2",
    "typescript": "^4.3.5",
    "webpack": "4.41.5",
    "webpack-dev-middleware": "3.7.2",
    "yargs": "17.0.1"
  }
}
