{
    "name": "web-playground",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "multi-static-dev --config ./configs/main.config.js",
        "build": "multi-static-build --config ./configs/main.config.js",
        "prettier": "prettier-eslint --write \"./**/*.{js,json,css,scss}\"",
        "lint": "eslint src/**/*.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "linters": {
            "src/**/*.{js}": [
                "prettier-eslint --write",
                "git add"
            ]
        },
        "concurrent": false
    },
    "devDependencies": {
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.23.0",
        "husky": "2.7.0",
        "lint-staged": "8.2.1",
        "prettier-eslint": "12.0.0",
        "prettier-eslint-cli": "5.0.1",
        "mocker-api": "1.11.2"
    },
    "dependencies": {
        "@babel/core": "7.14.2",
        "@babel/eslint-parser": "^7.14.2",
        "@babel/plugin-proposal-class-properties": "7.13.0",
        "@babel/plugin-transform-runtime": "7.14.2",
        "@babel/polyfill": "7.12.1",
        "@babel/preset-env": "7.14.2",
        "@babel/runtime": "7.14.0",
        "autoprefixer": "10.2.5",
        "babel-loader": "8.2.2",
        "core-js": "3",
        "fs-extra": "10.0.0",
        "glslify-loader": "2.0.0",
        "jquery": "3.6.0",
        "localhost-certs": "1.0.0",
        "lodash": "4.17.21",
        "multi-static": "0.1.3",
        "mustache": "4.2.0",
        "node-sass": "4.12.0",
        "postcss": "8.2.15",
        "raw-loader": "4.0.2",
        "read-first-line": "1.0.5",
        "static-hash-version": "1.0.0",
        "terser-webpack-plugin": "4.2.3",
        "webpack": "4.46.0",
        "webpack-dev-middleware": "3.7.3"
    }
}
