{
  "name": "web-playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "multi-static-dev --config ./configs/main.config.js",
    "build": "multi-static-build --config ./configs/main.config.js",
    "prettier": "prettier-eslint --write \"./**/*.{js,json,css,scss}\"",
    "lint": "eslint src/**/*.js",
    "postinstall": "node ./postinstall.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js}": [
        "prettier-eslint --write",
        "git add"
      ]
    },
    "concurrent": false
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.0",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "mocker-api": "1.11.2",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.1"
  },
  "dependencies": {
    "@babel/core": "7.8.4",
    "@babel/eslint-parser": "7.15.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@babel/runtime": "7.8.4",
    "autoprefixer": "9.7.6",
    "babel-loader": "8.2.2",
    "colors-cli": "1.0.27",
    "core-js": "3.15.2",
    "css-loader": "4.3.0",
    "fs-extra": "10.0.0",
    "glslify-loader": "^2.0.0",
    "jquery": "^3.6.0",
    "klaw-sync": "6.0.0",
    "localhost-certs": "1.0.0",
    "lodash": "4.17.21",
    "mocker-api": "1.11.2",
    "multi-static": "0.1.11",
    "mustache": "4.2.0",
    "node-sass": "4.12.0",
    "npm-install-subfolders": "0.1.2",
    "postcss": "8.3.5",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "4.2.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "promise-polyfill": "8.2.0",
    "raw-loader": "^4.0.2",
    "read-first-line": "1.0.4",
    "sass-loader": "10.2.0",
    "shelljs": "0.8.4",
    "static-hash-version": "1.0.0",
    "style-loader": "1.3.0",
    "terser-webpack-plugin": "2.3.4",
    "webpack": "4.41.5",
    "webpack-dev-middleware": "3.7.2",
    "yargs": "17.0.1"
  }
}
